//To create a ChessBoard Pattern
let str = "";
for(let i = 1; i <= 8 * 8; i++) {
	if((i%16 <= 8) && (i%16 != 0) ) {
  	  if(!(i%2))
        str += "#";
      else
        str += " ";
    }
  	else {
      if(!(i%2))
        str += " ";
      else 
        str += "#";
    }
  if(i%8==0) {
  	str += "\n"; 
  }
}
console.log(str);
